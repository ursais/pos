<?xml version="1.0" ?>
<odoo>

    <record id="pos_config_view_form_inherit_mebership_pos" model="ir.ui.view">
        <field name="name">pos.config.form.inherit.membership_pos</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//sheet/div[@id='title']" position="after">
                <group>
                    <group>
                        <field name="is_zipcode_required" />
                    </group>
                </group>
            </xpath>
        </field>
    </record>

</odoo>
